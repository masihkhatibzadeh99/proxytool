datadir="/var/lib/proxysql"

admin_variables=
{
    admin_credentials="admin:admin"
    mysql_ifaces="127.0.0.1:6032"
}

mysql_variables=
{
    threads=2
}

mysql_servers =
(
    { address = "10.0.0.10" , port = 3306 , hostgroup = 0 , max_connections = 200 }
)

mysql_users =
(
    { username = "proxy" , password = "secret" , default_hostgroup = 0 }
)

mysql_query_rules =
(
    { rule_id = 1 , match_pattern = "^SELECT" , destination_hostgroup = 0 , active = 1 }
)
